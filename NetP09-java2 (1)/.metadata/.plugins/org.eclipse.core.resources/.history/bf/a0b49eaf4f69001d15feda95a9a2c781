import java.awt.EventQueue;

import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.border.EmptyBorder;

import java.awt.BorderLayout;

public class UserProfile extends JFrame{

	private JFrame frame;
	private JPanel profilePane;
	private JPanel userImagePane;
	private JPanel usernamePane;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					UserProfile window = new UserProfile();
					window.frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the application.
	 */
	public UserProfile() {
		frame = new JFrame();
		frame.setBounds(200,100,300,50);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		profilePane = new JPanel();
		profilePane.setBounds(0,0,300,50);
		
		userImagePane = new JPanel();
		userImagePane.setBounds(0, 0, 50, 50);
		
		usernamePane = new JPanel();
		usernamePane.setBounds(50, 0, 250, 50);
		
		profilePane.setLayout(null);
		userImagePane.setLayout(null);
		usernamePane.setLayout(null);
		
		setContentPane(profilePane);
		profilePane.add(userImagePane);
		profilePane.add(usernamePane);


		
	}


}
